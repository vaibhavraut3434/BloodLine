
        #print(blood,"************")
        n =  obj.filter(city=city, state=state)
        #x = []
        if blood in "O Positive":
            for x in O_pos:
                #print(x)
                #print(n)
                n = n |  obj.filter(city=city, state=state,blood=x)
                #print(n)
                obj = testing1.objects.all()
                #y = n | x
            return n
        
        if blood in "O Negative":
            for x in O_neg:
                #print(x)
